{
  "metrics": {
    "gc": true,
    "memory": true,
    "address": {
      "path": "/metrics",
      "port": "9553",
      "ip": "0.0.0.0"
    }
  },
  "grpcSetting": {
    "enableOriginalProtoErrors": false,
    "proxyGrpcErrorDetails": false
  },
  "httpSetting": {
    "maxRequestBodySizeBytes": 0
  },
  "accountingSetting": {
    "enable": false,
    "setting": []
  },
  "redis": {
    "address": {
      "ip": "127.0.0.1",
      "port": "6379"
    }
  },
  "authCacheSetting": {
    "evictTimeout": "1m",
    "enableCash": false
  },
  "secrets": {
    "admin": "secret",
    "application": "SECRET_ACCESS_KEY",
    "verifyAppToken": false
  },
  "journalSetting": {
    "journal": {
      "rotateTimeoutMs": 86400000,
      "enableRemoteTransfer": true,
      "enable": false,
      "compress": true,
      "filename": "/var/log/isp-gate-service/runtime.log",
      "maxSizeMb": 512,
      "bufferSize": 4096
    },
    "methodsPatterns": []
  }
}
