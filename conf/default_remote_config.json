{
  "journal": {
    "enableRemoteTransfer": true,
    "enable": false,
    "compress": true,
    "filename": "/var/log/isp-gate-service/runtime.log",
    "maxSizeMb": 512,
    "bufferSize": 4096,
    "rotateTimeoutMs": 86400000
  },
  "metrics": {
    "gc": true,
    "memory": true,
    "address": {
      "path": "/metrics",
      "port": "9553",
      "ip": "0.0.0.0"
    }
  },
  "secretKey": "secret key",
  "grpcSetting": {},
  "serverSetting": {},
  "accountingSetting": {
    "enable": false,
    "setting": [
      {
        "limits": [
          {
            "pattern": "/healthcheck/module/group/method",
            "timeout": "1s",
            "maxCount": 1000
          }
        ],
        "applicationId": 1
      }
    ]
  },
  "redis": {
    "address": {
      "ip": "127.0.0.1",
      "port": "6379"
    }
  }
}